{
  "name": "@jvdx/eslint-config",
  "version": "4.5.0",
  "description": "jvdx's eslint configuration",
  "author": "Joel Voss <mail@joelvoss.com>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/joelvoss/jvdx-eslint-config",
  "bugs": {
    "url": "https://github.com/joelvoss/jvdx-eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelvoss/jvdx-eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "eslint-plugin",
    "eslint",
    "jvdx"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "./src/index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write src",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@eslint/js": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0"
  },
  "devDependencies": {
    "@jvdx/prettier-config": "^0.3.4",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.26.0 || ^9.0.0",
    "react": "^18.0.0",
    "typescript": ">=3.3.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "prettier": "@jvdx/prettier-config"
}
